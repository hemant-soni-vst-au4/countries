    "build:prod": "npm run build -- --prod",
    "deploy:web-stg": "aws s3 sync --profile sen-deploy-user --acl public-read ./build s3://sen-webapp-stg && aws s3 cp --profile sen-deploy-user --cache-control \"max-age=0,no-cache,no-store,must-revalidate\" --acl public-read ./build/ngsw-worker.js s3://sen-webapp-stg",
    "deploy:web-live": "aws s3 sync --profile sen-deploy-user --acl public-read ./build s3://hemant-webapp && aws s3 cp --profile sen-deploy-user --cache-control \"max-age=0,no-cache,no-store,must-revalidate\" --acl public-read ./build/ngsw-worker.js s3://hemant-webapp",
    "invalid:web-stg": "aws cloudfront create-invalidation --distribution-id E3UYZUQV95X8JE --paths \"/*\" --profile sen-deploy-user",
    "invalid:web-live": "aws cloudfront create-invalidation --distribution-id E1WW51YO646YER --paths \"/*\" --profile sen-deploy-user"
